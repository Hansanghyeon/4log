{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@api/*": [
        "src/lib/api/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@src/*": [
        "src/*"
      ],
      "@/*": [
        "src/components/*"
      ],
      "@atom/*": [
        "src/components/atoms/*"
      ],
      "@module/*": [
        "src/components/modules/*"
      ],
      "@template/*": [
        "src/components/templates/*"
      ],
      "@style/*": [
        "src/styles/*"
      ],
      "@hook/*": [
        "src/hook/*"
      ],
      "@store/*": [
        "src/store/*"
      ]
    }
  }
}